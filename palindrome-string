/**Examples :
Input: s = "abba"
Output: true
Explanation: "abba" reads the same forwards and backwards, so it is a palindrome.
**/

isPalindrome(s) {
        let val=true;
       let st=0,end=s.length-1,i=0;
    while(st<end){
       if(s.charAt(st)!= s.charAt(end)){
          val=false;
          break;
       }
        st++;
        end--;
    }
    return val;
    }
